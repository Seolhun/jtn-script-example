{
  "presets": [
    [
      "@babel/preset-env",
      "@babel/preset-es2015",
      "@babel/preset-react",
      "@babel/preset-stage-2",
      "@babel/polyfill",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        }
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-es2015",
        "@babel/preset-react",
        "@babel/preset-stage-2",
        "@babel/polyfill"
      ]
    }
  },
  "plugins": ["styled-components"]
}
