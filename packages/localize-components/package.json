{
  "name": "@seolhun/localize-components",
  "description": "Localizing React components is rendered by stand-alone not affect other styles.",
  "version": "0.0.9",
  "license": "MIT",
  "main": "./src/index.ts",
  "files": [
    "dist/",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "node build/build.js",
    "dev": "node build/dev-server.js",
    "bundle": "yarn style:local && yarn style && tsc && yarn style:copy",
    "style": "sass --update src:../../dist/packages/localize-components/src --style compressed",
    "style:watch": "sass --watch src:dist/src --style compressed",
    "style:local": "sass --update src:src --style compressed",
    "style:copy": "copyfiles src/**/*.css dist/",
    "style:clean": "find . -name *.css* -type f -delete",
    "test": "jest",
    "test-with-coverage": "jest --coverage && cat ./coverage/lcov.info | codacy-coverage"
  },
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@gfx/zopfli": "^1.0.9",
    "@seolhun/localize-components-types": "^0.0.9",
    "@seolhun/localize-components-utils": "^0.0.9",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.8.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^2.0.0",
    "copyfiles": "^2.1.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eventsource-polyfill": "^0.9.6",
    "filemanager-webpack-plugin": "^2.0.5",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.0",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "opn": "^5.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^3.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "sass": "^1.14.3",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.2",
    "style-loader": "^0.23.1",
    "styled-components": "^3.4.6",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6",
    "webpack": "^4.24.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.4"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "sass-loader": "^7.1.0"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  }
}
