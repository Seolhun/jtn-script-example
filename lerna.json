{
  "version": "0.5.2",
  "packages": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "allowBranch": [
        "master",
        "develop"
      ],
      "ignoreChanges": [
        "packages/templates",
        "**/__test__",
        "**/*.md"
      ],
      "ignore": [
        "packages/docs",
        "packages/examples",
        "packages/templates"
      ]
    },
    "bootstrap": {
      "ignore": [
        "packages/docs",
        "packages/examples",
        "packages/templates"
      ]
    }
  }
}
